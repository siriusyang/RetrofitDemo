<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonProgressBar">
        <attr name="bgColor" format="color" />
        <attr name="progColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="text" format="string" />
        <attr name="type" format="enum">
            <enum name="determinate" value="0" />
            <enum name="indeterminate" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="BezierRoundView">
        <attr name="color_bez" format="color"/>
        <attr name="color_touch" format="color"/>
        <attr name="color_stroke" format="color"/>
        <attr name="time_animator" format="integer"/>
        <attr name="round_count" format="integer"/>
        <attr name="radius" format="dimension"/>

    </declare-styleable>
</resources>